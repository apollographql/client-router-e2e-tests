{
  "name": "vue-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "router:up": "cd ../scripts && make docker-build-local-router && make docker-up-local-router && cd ../vue-demo",
    "router:down": "cd ../scripts && make docker-compose-down && cd ../vue-demo"
  },
  "dependencies": {
    "@apollo/client": "3.7.0-beta.7",
    "@vue/apollo-composable": "4.0.0-alpha.19",
    "core-js": "3.8.3",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "vue": "3.2.13"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/eslint-parser": "7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
